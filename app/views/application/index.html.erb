  <% @items.each do |item| %>
    <%= image_tag item.url, class: 'draggable', alt: item.name %>
  <% end %>

<section>
  <div id="mood-container">
  </div>
</section>

<section class="prompt" id='<%= @prompt.id %>'>
  <%= @prompt.text %>
</section>

<section class="forms">
  <form id="mood_form">
    <h5>Save ur mood</h5>
    <input type='text' id='mood_name' placeholder='Title'/>
    <input type='text' id='mood_description' placeholder='Description'/>
    <button type='submit' id='mood_button' class='button' >YES</button>
  </form>
</section>
